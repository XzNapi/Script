-- state
local spyEnabled = false
local spyLogEntries = {}
local logVault = {}
local selectedText = nil

-- util
local function serializeArgs(args)
    local t = {}
    for _, v in ipairs(args) do
        local typ = typeof(v)
        if typ == "Instance" then
            table.insert(t, 'game:FindFirstChild("'..v:GetFullName()..'", true)')
        elseif typ == "string" then
            table.insert(t, '"'..v:gsub('"', '\\"')..'"')
        elseif typ == "CFrame" or typ == "Vector3" then
            table.insert(t, "CFrame.new"..tostring(v))
        else
            table.insert(t, tostring(v))
        end
    end
    return table.concat(t, ", ")
end

local function addSpyLine(line)
    table.insert(spyLogEntries, line)
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 0, 18)
    label.BackgroundTransparency = 1
    label.Text = line
    label.Font = Enum.Font.Code
    label.TextSize = 11
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.TextColor3 = Color3.fromRGB(255,255,255)
    label.TextTruncate = Enum.TextTruncate.AtEnd
    label.Parent = spyScroll
    spyLayout:ApplyLayout()
    spyScroll.CanvasSize = UDim2.new(0,0,0, spyLayout.AbsoluteContentSize.Y + 10)

    label.InputBegan:Connect(function(inp)
        if inp.UserInputType == Enum.UserInputType.MouseButton1 then
            selectedText = line
            for _, v in ipairs(spyScroll:GetChildren()) do
                if v:IsA("TextLabel") then v.BackgroundTransparency = 1 end
            end
            label.BackgroundTransparency = 0.5
            label.BackgroundColor3 = Color3.fromRGB(60,60,60)
        end
    end)
end

-- Script Spy tanpa hookfunction â€“ pakai __namecall metatable

local RealRemoteEvent   = Instance.new("RemoteEvent").FireServer
local RealRemoteFunc    = Instance.new("RemoteFunction").InvokeServer
local selectedText      = nil

local function serializeArgs(args)
    local t = {}
    for _, v in ipairs(args) do
        local typ = typeof(v)
        if typ == "Instance" then
            t[#t+1] = 'game:FindFirstChild("'..v:GetFullName()..'", true)'
        elseif typ == "string" then
            t[#t+1] = '"'..v:gsub('"', '\\"')..'"'
        elseif typ == "CFrame" or typ == "Vector3" then
            t[#t+1] = "CFrame.new"..tostring(v)
        else
            t[#t+1] = tostring(v)
        end
    end
    return table.concat(t, ", ")
end

local function addLog(remoteType, remote, ...)
    if not spyEnabled then return end
    local line = string.format('game:GetService("ReplicatedStorage")["%s"]:%s(%s)',
                               remote.Name, remoteType, serializeArgs({...}))
    addSpyLine(line)
end

-- hook __namecall
local mt = getrawmetatable(game)
local oldNamecall = mt.__namecall
setreadonly(mt, false)

mt.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    if method == "FireServer" then
        addLog("FireServer", self, ...)
    elseif method == "InvokeServer" then
        addLog("InvokeServer", self, ...)
    end
    return oldNamecall(self, ...)
end)

setreadonly(mt, true)

-- tombol toggle (pastikan sudah ada di daftar createButton)
local spyBtn = createButton("Script Spy: OFF")
spyBtn.MouseButton1Click:Connect(function()
    spyEnabled = not spyEnabled
    spyBtn.Text = spyEnabled and "Script Spy: ON" or "Script Spy: OFF"
    spyFrame.Visible = spyEnabled
end)

-- tombol clear
clearBtn.MouseButton1Click:Connect(function()
    for _, v in ipairs(spyScroll:GetChildren()) do
        if v:IsA("TextLabel") then v:Destroy() end
    end
    spyLogEntries = {}
    spyScroll.CanvasSize = UDim2.new(0,0,0,0)
end)

-- copy & exec
local function toClipboard(str)
    if setclipboard then setclipboard(str)
    elseif syn and syn.write_clipboard then syn.write_clipboard(str)
    else warn("Clipboard tidak tersedia") end
end

copyBtn.MouseButton1Click:Connect(function()
    if selectedText then toClipboard(selectedText) end
end)

execBtn.MouseButton1Click:Connect(function()
    if selectedText then
        local fn, err = loadstring(selectedText)
        if fn then fn() else warn("Exec gagal: "..err) end
    end
end)

-- vault save & refresh
local function refreshVault()
    for _, v in ipairs(vaultScroll:GetChildren()) do
        if v:IsA("TextLabel") then v:Destroy() end
    end
    for idx, txt in ipairs(logVault) do
        local vLbl = Instance.new("TextLabel")
        vLbl.Size = UDim2.new(1, 0, 0, 18)
        vLbl.BackgroundTransparency = 1
        vLbl.Text = idx..") "..txt:sub(1,45).."..."
        vLbl.Font = Enum.Font.Code
        vLbl.TextSize = 10
        vLbl.TextXAlignment = Enum.TextXAlignment.Left
        vLbl.TextColor3 = Color3.fromRGB(200,200,200)
        vLbl.Parent = vaultScroll
        vLbl.InputBegan:Connect(function(inp)
            if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                addSpyLine(txt)
                selectedText = txt
            end
        end)
    end
    vaultLayout:ApplyLayout()
    vaultScroll.CanvasSize = UDim2.new(0,0,0, vaultLayout.AbsoluteContentSize.Y + 10)
end

saveVaultBtn.MouseButton1Click:Connect(function()
    for _, txt in ipairs(spyLogEntries) do
        table.insert(logVault, txt)
    end
    refreshVault()
end)

