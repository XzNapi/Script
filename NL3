-- =========================================================
--  SIMPLE SOURCE-CODE LOGGER (no hookfunction / no LogService)
--  Works on Synapse/Electron/Krnl/Hydrogen/Fluxus/etc
-- =========================================================
local stealSource = true            -- toggle
local stealSource = true      -- toggle
local captured = {}           -- [name] = source
local MAX_CAPTURE = 100

local function logSource(src, name)
    if not stealSource or not src then return end
    if #captured >= MAX_CAPTURE then table.remove(captured, 1) end
    captured[name or ("Untitled_" .. #captured+1)] = src
end

-- hook loadstring ------------------------------------------------
local real_loadstring = loadstring
if real_loadstring then
    _G.loadstring = newcclosure(function(src)
        logSource(src, "loadstring_" .. #captured+1)
        return real_loadstring(src)
    end)
end

-- hook require ----------------------------------------------------
local real_require = require
if real_require then
    _G.require = newcclosure(function(mod)
        local info = debug.getinfo(mod, "s")
        if info and info.source then
            logSource(info.source, "require_" .. tostring(mod))
        end
        return real_require(mod)
    end)
end

-- hook dofile -----------------------------------------------------
local real_dofile = dofile
if real_dofile then
    _G.dofile = newcclosure(function(path)
        local src = readfile(path)
        logSource(src, path)
        return real_dofile(path)
    end)
end

