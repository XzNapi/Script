-- 3.  SPY SCRIPT GUI
------------------------------------------------------------------
local gui          = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("NLightV1")
local UIS          = game:GetService("UserInputService")

-- Frame utama ----------------------------------------------------
local spyFrame = gui:FindFirstChild("ScriptSpyFrame")
if not spyFrame then
    spyFrame = Instance.new("Frame")
    spyFrame.Name = "ScriptSpyFrame"
    spyFrame.Size = UDim2.new(0, 550, 0, 320)
    spyFrame.Position = UDim2.new(0.5, -275, 0.5, -160)
    spyFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    spyFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    spyFrame.Visible = false
    spyFrame.Active = true
    Instance.new("UICorner", spyFrame).CornerRadius = UDim.new(0, 10)
    spyFrame.Parent = gui
end

-----------------------------------------------
-- Title -----------
-----------------------------------------------
local spyTitle = spyFrame:FindFirstChild("Title") or Instance.new("TextLabel")
spyTitle.Name = "Title"
spyTitle.Size = UDim2.new(1, 0, 0, 30)
spyTitle.Text = "Script Spy"
spyTitle.Font = Enum.Font.GothamBold
spyTitle.TextSize = 16
spyTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
spyTitle.BackgroundTransparency = 1
spyTitle.Parent = spyFrame

------------------------------------------------------------------
--  Drag Handler  (baru, pakai title bar)
------------------------------------------------------------------
local dragHandle = spyTitle   -- sekarang spyTitle sudah ada
local dragStart, startPos

-- Mulai drag
dragHandle.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragStart = input.Position
        startPos  = spyFrame.Position
        local conn
        conn = input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                conn:Disconnect()
            end
        end)
    end
end)

-- Gerakkan
UIS.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragStart then
        local delta = input.Position - dragStart
        spyFrame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)


