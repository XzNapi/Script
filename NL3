-- Tombol Copy / Exec / Clear / Refresh
local function createBtn(text, color)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0.25, -4, 1, 0)
    btn.Text = text
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 13
    btn.BackgroundColor3 = color
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)
    btn.Parent = btnBar
    return btn
end

local copyBtn   = createBtn("Copy",  Color3.fromRGB(80, 120, 255))
local execBtn   = createBtn("Exec",  Color3.fromRGB(80, 180, 80))
local clearBtn  = createBtn("Clear", Color3.fromRGB(180, 80, 80))
local refreshBtn= createBtn("Refresh",Color3.fromRGB(100,180,100))

-- Right panel ----------------------------------------------------
local right = main:FindFirstChild("Right") or Instance.new("Frame")
right.Name = "Right"
right.Size = UDim2.new(0.4, 0, 1, 0)
right.BackgroundTransparency = 1
right.Parent = main
local vaultScroll = right:FindFirstChild("VaultScroll") or Instance.new("ScrollingFrame")
vaultScroll.Name = "VaultScroll"
vaultScroll.Size = UDim2.new(1, 0, 1, -35)
vaultScroll.BackgroundTransparency = 1
vaultScroll.ScrollBarThickness = 6
vaultScroll.CanvasSize = UDim2.new(0, 0, 0, 1000)
vaultScroll.Parent = right
local vaultLayout = right:FindFirstChild("VaultLayout") or Instance.new("UIListLayout")
vaultLayout.Name = "VaultLayout"
vaultLayout.Padding = UDim.new(0, 2)
vaultLayout.Parent = vaultScroll

-- Utility clipboard
local function toClip(str)
    if setclipboard then setclipboard(str)
    elseif syn and syn.write_clipboard then syn.write_clipboard(str)
    else warn("Clipboard not supported") end
end

-- Refresh panel
local function refreshSourcePanel()
    for _,v in ipairs(vaultScroll:GetChildren()) do
        if v:IsA("TextButton") then v:Destroy() end
    end
    for name,src in pairs(captured or {}) do
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 22)
        btn.Text = name
        btn.Font = Enum.Font.Code
        btn.TextSize = 12
        btn.TextColor3 = Color3.fromRGB(255,255,255)
        btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
        Instance.new("UICorner", btn).CornerRadius = UDim.new(0,4)
        btn.Parent = vaultScroll
        btn.MouseButton1Click:Connect(function()
            liveLabel.Text = src
            liveScroll.CanvasSize = UDim2.new(0,0,0,liveLabel.TextBounds.Y+10)
            selectedText = src
        end)
    end
    vaultLayout:ApplyLayout()
end

-- Event tombol
copyBtn.MouseButton1Click:Connect(function()
    if selectedText then toClip(selectedText) end
end)
execBtn.MouseButton1Click:Connect(function()
    if selectedText then
        local fn,err = loadstring(selectedText)
        if fn then fn() else warn("Exec failed: "..err) end
    end
end)
clearBtn.MouseButton1Click:Connect(function()
    captured = {}
    refreshSourcePanel()
    liveLabel.Text = "-- klik nama di kanan --"
end)
refreshBtn.MouseButton1Click:Connect(function()
    refreshSourcePanel()
end)

-- Auto-refresh setiap 1 detik
task.spawn(function()
    while task.wait(1) do
        refreshSourcePanel()
    end
end)

------------------------------------------------------------------
-- 5.  TOGGLE BUTTON & HOTKEY
------------------------------------------------------------------
local spyBtn = createButton("Script Spy: OFF")
spyBtn.MouseButton1Click:Connect(function()
    spyFrame.Visible = not spyFrame.Visible
    spyBtn.Text = spyFrame.Visible and "Script Spy: ON" or "Script Spy: OFF"
end)

-- Hotkey RightShift
UIS.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.RightShift then
        spyFrame.Visible = not spyFrame.Visible
        spyBtn.Text = spyFrame.Visible and "Script Spy: ON" or "Script Spy: OFF"
    end
end)


-- GUI Management (Minimize, Open, Delete)
local minimizedBtn = Instance.new("TextButton", gui)
minimizedBtn.Size = UDim2.new(0, 120, 0, 30)
minimizedBtn.Position = UDim2.new(0, 10, 0, 10)
minimizedBtn.Text = "Open NLight"
minimizedBtn.Font = Enum.Font.GothamBold
minimizedBtn.TextSize = 14
minimizedBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
minimizedBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
minimizedBtn.Visible = false
minimizedBtn.Active = true
minimizedBtn.Draggable = true
Instance.new("UICorner", minimizedBtn).CornerRadius = UDim.new(0, 10)

print("NLight V.1 + FULL SpyScript loaded â€“ RightShift or button to toggle GUI.")

minimize.MouseButton1Click:Connect(function()
	frame.Visible = false
	minimizedBtn.Visible = true
	if tpFrame then tpFrame.Visible = false end
	if speedFrame then speedFrame.Visible = false end
	if pumpFrame then pumpFrame.Visible = false end
	if waypointFrame then 
        waypointFrame.Visible = false 
        saveFrame.Visible = false
        useFrame.Visible = false
    end
end)

minimizedBtn.MouseButton1Click:Connect(function()
	frame.Visible = true
	minimizedBtn.Visible = false
	if tpEnabled and tpFrame then tpFrame.Visible = true end
	if speedEnabled and speedFrame then speedFrame.Visible = true end
	if autoPumpEnabled and pumpFrame then pumpFrame.Visible = true end
	if waypointEnabled and waypointFrame then waypointFrame.Visible = true end
end)

delete.MouseButton1Click:Connect(function()
    autoPumpEnabled = false 
    gui:Destroy()
end)
